@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}
<style>
   

    h1 {
        color: red;
    }

    h6 {
        color: red;
        font-weight: 100;
        text-decoration: none; /*underline;*/
    }

</style>
<div class="container-fluid">
    <div class="card o-hidden border-0 shadow-lg my-7">
        <div class="card-body p-0" style="resize:initial">
            <!-- Nested Row within Card Body -->


                <div class="card-body">

   
            <div class="w3-display-middle">
                <h1 class="w3-jumbo w3-animate-top w3-center"><code>Access Denied</code></h1>
                <hr class="w3-border-white w3-animate-left" style="margin:auto;width:100%">
                <h3 class="w3-center w3-animate-right">You dont have permission to view this site.</h3>
                <h3 class="w3-center w3-animate-zoom">🚫🚫🚫🚫</h3>
                <h6 class="w3-center w3-animate-zoom"><strong>Error Code</strong>: 403 forbidden</h6>
            </div>

</div>
</div>
</div>
</div>




